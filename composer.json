{
    "name": "greys/wc-subscriptions-tests",
    "description": "PHPUnit test framework and helpers for WooCommerce Subscriptions extensions",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "jgreys",
            "homepage": "https://github.com/jgreys"
        }
    ],
    "keywords": [
        "woocommerce",
        "woocommerce-subscriptions",
        "phpunit",
        "testing",
        "test-framework"
    ],
    "require": {
        "php": ">=7.4",
        "greys/woocommerce-phpunit-framework": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Greys\\WooCommerce\\Subscriptions\\Tests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greys\\WooCommerce\\Subscriptions\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}